@{
    ViewData["Title"] = "Header";
}
<!DOCTYPE html>
<html>
<head>
    <title>UniCalendar</title>
    <link rel="stylesheet" href="~/css/header.css"/>
    <link rel="icon" href="~/images/calendar.png" type="image/x-icon">
    <script async defer src="https://apis.google.com/js/api.js" onload="gapiLoaded()"></script>
    <script async defer src="https://accounts.google.com/gsi/client" onload="gisLoaded()"></script>
    <script async defer src="~/js/Authorize.js"></script>
</head>
<body>
    <div class="cssHeader">
        @await Html.PartialAsync("Burgermenu")
        <div class="cssEgrafi">
            <span style="width: 30%; top: 10px; left: 33%; position: absolute;">Εγγραφή</span>
        </div>
        <div class="cssSindesi">
            <button id="authorize_button" onclick="handleAuthClick()">Sign In</button>
            <button id="signout_button" onclick="handleSignoutClick()">Sign Out</button>
        </div>
        <div class="cssUnicalendar">
            <span style="width: 65px; top: 3px; left: 49px; position: absolute;">UniCalendar</span>
            <div class="cssLogo"></div>
        </div>
    </div>
</body>
</html>